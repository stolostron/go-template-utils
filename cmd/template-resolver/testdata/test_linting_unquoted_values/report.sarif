{
  "version": "2.1.0",
  "$schema": "https://raw.githubusercontent.com/oasis-tcs/sarif-spec/main/sarif-2.1/schema/sarif-schema-2.1.0.json",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "go-template-utils-linter",
          "informationUri": "https://github.com/stolostron/go-template-utils",
          "rules": [
            {
              "id": "GTUL002",
              "name": "mismatchedDelimiters",
              "shortDescription": {
                "text": "Template delimiters must be properly paired."
              },
              "fullDescription": {
                "text": "Template start delimiters (`{{` or `{{hub`) must be paired with a corresponding closing delimiter (`}}` or `hub}}`). Otherwise, the template is invalid.",
                "markdown": "Template start delimiters (`{{` or `{{hub`) must be paired with a corresponding closing delimiter (`}}` or `hub}}`). Otherwise, the template is invalid."
              }
            },
            {
              "id": "GTUL003",
              "name": "unquotedTemplateValues",
              "shortDescription": {
                "text": "Template expressions should be single-quoted."
              },
              "fullDescription": {
                "text": "Single-quote wrapping around template expressions in YAML will ensure that the templates are properly interpreted and not interfere with the rest of the structure.",
                "markdown": "Single-quote wrapping around template expressions in YAML will ensure that the templates are properly interpreted and not interfere with the rest of the structure."
              }
            }
          ]
        }
      },
      "artifacts": [
        {
          "location": {
            "uri": "cmd/template-resolver/testdata/test_linting_unquoted_values/input.yaml"
          }
        }
      ],
      "results": [
        {
          "level": "warning",
          "message": {
            "text": "Templates should be single-quoted."
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "cmd/template-resolver/testdata/test_linting_unquoted_values/input.yaml",
                  "index": 0
                },
                "region": {
                  "startLine": 21,
                  "startColumn": 25
                }
              }
            }
          ],
          "ruleId": "GTUL003",
          "ruleIndex": 1
        },
        {
          "level": "warning",
          "message": {
            "text": "Templates should be single-quoted."
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "cmd/template-resolver/testdata/test_linting_unquoted_values/input.yaml",
                  "index": 0
                },
                "region": {
                  "startLine": 23,
                  "startColumn": 32
                }
              }
            }
          ],
          "ruleId": "GTUL003",
          "ruleIndex": 1
        },
        {
          "level": "warning",
